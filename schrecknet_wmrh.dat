lings": []}, {"id": 200935, "name": "Maris Streck (G3)", "printed_name": "Maris Streck", "url": "https://static.krcg.org/card/marisstreckg3.jpg", "group": "3", "capacity": 9, "types": ["Vampire"], "clans": ["Malkavian"], "disciplines": ["ani", "dem", "dom", "AUS", "OBF"], "text": "Camarilla Malkavian Justicar: Maris can burn a blood to give a blocking minion +1 intercept. She can take a +1 stealth action to allow you to look at and rearrange the top 5 cards of your library.", "sets": [{"name": "Camarilla Edition", "picUrl": "https://static.krcg.org/card/set/camarilla-edition/marisstreckg3.jpg"}, {"name": "Blood Shadowed Court", "picUrl": "https://static.krcg.org/card/set/blood-shadowed-court/marisstreckg3.jpg"}], "rulings": [{"text": "Can use her ability multiple times each action. [LSJ 20020814] [ANK 20180307-2]", "refs": [{"ref": "LSJ 20020814", "url": "https://groups.google.com/g/rec.games.trading-cards.jyhad/c/gt8wQhk76lA/m/qjDJXRLxM7cJ"}, {"ref": "ANK 20180307-2", "url": "https://www.vekn.net/forum/rules-questions/76451-ellison-humboldt-and-matteus-flesh-sculptor?start=0#85598"}]}, {"text": "Can use her ability while locked. [ANK 20220705]", "refs": [{"ref": "ANK 20220705", "url": "https://www.vekn.net/forum/rules-questions/79895-question-regarding-using-a-minion-card-text-ability-when-locked#105630"}]}]}, {"id": 200936, "name": "Mark (G2)", "printed_name": "Mark", "url": "https://static.krcg.org/card/markg2.jpg", "group": "2", "capacity": 6, "types": ["Vampire"], "clans": ["Blood Brother"], "disciplines": ["for", "obf", "pot", "SAN"], "text": "Sabbat bishop. Torrance Circle: Mark gets +1 intercept when attempting to block a bleed action. Sterile.", "sets": [{"name": "Bloodlines", "picUrl": "https://static.krcg.org/card/set/bloodlines/markg2.jpg"}], "rulings": []}, {"id": 200937, "name": "Mark Decker (G3)", "printed_name": "Mark Decker", "url": "https://static.krcg.org/card/markdeckerg3.jpg", "group": "3", "capacity": 8, "types": ["Vampire"], "clans": ["Gangrel"], "disciplines": ["ani", "for", "obf", "pot", "CEL", "PRO"], "text": "Camarilla Prince of Milwaukee: Mark gets an additional +1 strength in combat with a werewolf. +1 strength.", "sets": [{"name": "The Unaligned", "picUrl": "https://static.krcg.org/card/set/the-unaligned/markdeckerg3.jpg"}, {"name": "Twenty-Fifth Anniversary", "picUrl": "https://static.krcg.org/card/set/twenty-fifth-anniversary/markdeckerg3.jpg"}], "rulings": []}, {"id": 200938, "name": "Marla Kenyon (G4)", "printed_name": "Marla Kenyon", "url": "https://static.krcg.org/card/marlakenyong4.jpg", "group": "4", "capacity": 4, "types": ["Vampire"], "clans": ["Ministry"], "disciplines": ["ser", "PRE"], "text": "Independent.", "sets": [{"name": "Lords of the Night", "picUrl": "https://static.krcg.org/card/set/lords-of-the-night/marlakenyong4.jpg"}, {"name": "Print on Demand", "picUrl": "https://static.krcg.org/card/set/print-on-demand/marlakenyong4.jpg"}], "rulings": []}, {"id": 200939, "name": "Marlena (G3)", "printed_name": "Marlena", "url": "https://static.krcg.org/card/marlenag3.jpg", "group": "3", "capacity": 5, "types": ["Vampire"], "clans": ["Brujah"], "disciplines": ["cel", "pre", "POT"], "text": "Camarilla: Marlena does not lock when performing a recruit ally action. She can perform only 1 recruit ally action each turn.", "sets": [{"name": "Camarilla Edition", "picUrl": "https://static.krcg.org/card/set/camarilla-edition/marlenag3.jpg"}, {"name": "Blood Shadowed Court", "picUrl": "https://static.krcg.org/card/set/blood-shadowed-court/marlenag3.jpg"}], "rulings": []}, {"id": 200940, "name": "Marlene, The Infernalist (G2)", "printed_name": "Marlene, The Infernalist", "url": "https://static.krcg.org/card/marlenetheinfernalistg2.jpg", "group": "2", "capacity": 6, "types": ["Vampire"], "clans": ["Ventrue antitribu"], "disciplines": ["dem", "for", "tha", "AUS", "DOM"], "text": "Sabbat.", "sets": [{"name": "Sabbat", "picUrl": "https://static.krcg.org/card/set/sabbat/marlenetheinfernalistg2.jpg"}, {"name": "Sabbat War", "picUrl": "https://static.krcg.org/card/set/sabbat-war/marlenetheinfernalistg2.jpg"}], "rulings": []}, {"id": 200941, "name": "Marta (G4)", "printed_name": "Marta", "url": "https://static.krcg.org/card/martag4.jpg", "group": "4", "capacity": 3, "types": ["Vampire"], "clans": ["Malkavian antitribu"], "disciplines": ["aus", "dem"], "text": "Sabbat: When Marta diablerizes an older vampire, she gains a blood (after receiving a master: Discipline card, if any).", "sets": [{"name": "Third Edition", "picUrl": "https://static.krcg.org/card/set/third-edition/martag4.jpg"}], "rulings": []}, {"id": 200942, "name": "Marthe Dizier (G3)", "printed_name": "Marthe Dizier", "url": "https://static.krcg.org/card/marthedizierg3.jpg", "group": "3", "capacity": 10, "types": ["Vampire"], "clans": ["Toreador antitribu"], "disciplines": ["pro", "AUS", "CEL", "OBF", "PRE"], "text": "Sabbat. Black Hand: When a combat involving Marthe ends, she can burn a blood to move a card she played during that combat from your ash heap to your hand. +1 bleed.", "sets": [{"name": "Gehenna", "picUrl": "https://static.krcg.org/card/set/gehenna/marthedizierg3.jpg"}], "rulings": [{"text": "Her ability can only move cards from the ash heap that were played in the normal manner. If the card was put in play in some other way, it cannot be retrieved by her special. [LSJ 20041119]", "refs": [{"ref": "LSJ 20041119", "url": "https://groups.google.com/g/rec.games.trading-cards.jyhad/c/HMV_pN5mJhQ/m/tzkinpwUq-MJ"}]}, {"text": "A card canceled as it is played is still considered played, she can retrieve it. [LSJ 19980212] [ANK 20190701]", "refs": [{"ref": "LSJ 19980212", "url": "https://groups.google.com/g/rec.games.trading-cards.jyhad/c/fLFLlXZXHqA/m/ggjw8aGjdRoJ"}, {"ref": "ANK 20190701", "url": "https://www.vekn.net/forum/rules-questions/77763-multiple-questions#95690"}]}, {"text": "She can retrieve {Psyche!} (reversal of previous rulings). [PIB 20111208]", "refs": [{"ref": "PIB 20111208", "url": "https://www.vekn.net/forum/rules-questions/16769-rules-team-rulings-02-dec-11?limit=10&start=40#17547"}]}, {"text": "Black Hand is not a title, it is a trait unrelated to any sect. The trait is not lost if the vampire changes sect. [LSJ 20070322] [ANK 20180807]", "refs": [{"ref": "LSJ 20070322", "url": "https://groups.google.com/g/rec.games.trading-cards.jyhad/c/Ww-4rYJxi4w/m/P3QchWVq2o4J"}, {"ref": "ANK 20180807", "url": "https://www.vekn.net/forum/rules-questions/76905-going-anarch-as-black-hand#89735"}]}]}, {"id": 200943, "name": "Martin Franckel (G3)", "printed_name": "Martin Franckel", "url": "https://static.krcg.org/card/martinfranckelg3.jpg", "group": "3", "capacity": 3, "types": ["Vampire"], "clans": ["Tremere"], "disciplines": ["tha", "AUS"], "text": "Camarilla: Martin gets -1 bleed when bleeding a Methuselah who controls neither a ready Tremere nor a ready Tremere antitribu.", "sets": [{"name": "Anarchs", "picUrl": "https://static.krcg.org/card/set/anarchs/martinfranckelg3.jpg"}], "rulings": []}, {"id": 201601, "name": "Martina Srnankova (G6)", "printed_name": "Martina Srnankova", "url": "https://static.krcg.org/card/martinasrnankovag6.jpg", "group": "6", "capacity": 6, "types": ["Vampire"], "clans": ["Gangrel"], "disciplines": ["ani", "FOR", "PRO"], "text": "Anarch: Martina gets +1 intercept during bleed actions.", "sets": [{"name": "Fifth Edition (Anarch)", "picUrl": "https://static.krcg.org/card/set/fifth-edition-anarch/martinasrnankovag6.jpg"}], "rulings": []}, {"id": 200944, "name": "Martino della Passaglia (G6)", "printed_name": "Martino della Passaglia", "url": "https://static.krcg.org/card/martinodellapassagliag6.jpg", "group": "6", "capacity": 7, "types": ["Vampire"], "clans": ["Giovanni"], "disciplines": ["obf", "DOM", "NEC", "POT"], "text": "Independent: Equipment costs Martino 1 fewer blood or pool to equip.", "sets": [{"name": "The Unaligned", "picUrl": "https://static.krcg.org/card/set/the-unaligned/martinodellapassagliag6.jpg"}], "rulings": 